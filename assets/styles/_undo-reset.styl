/* generic block-level elements */

p {
  display: block;
  margin-top: 1.125rem;
  margin-bottom: 1.125rem;
  margin-left: 0;
  margin-right: 0;
}

blockquote {
  display: block;
  margin-top: 1.125rem;
  margin-bottom: 1em;
  margin-left: 40px;
  margin-right: 40px;
}

figcaption {
  display: block;
}

figure {
  display: block;
  margin-top: 1em;
  margin-bottom: 1em;
  margin-left: 40px;
  margin-right: 40px;
}

q {
  display: inline;
}

q::before {
  content: open-quote;
}

q::after {
  content: close-quote;
}

center {
  display: block;
  /* special centering to be able to emulate the html4/netscape behaviour */
  text-align: center;
}

hr {
  display: block;
  margin-top: 0.5em;
  margin-bottom: 0.5em;
  margin-left: auto;
  margin-right: auto;
  border-style: inset;
  border-width: 1px;
}

/* heading elements */

h1 {
  display: block;
  font-size: 2em;
  margin-top: 0.67rem;
  margin-bottom: 0.67em;
  margin-left: 0;
  margin-right: 0;
  font-weight: bold;
}

:matches(article, aside, nav, section) h1 {
  font-size: 1.5em;
  margin-top: 0.83rem;
  margin-bottom: 0.83em;
}

:matches(article, aside, nav, section) :matches(article, aside, nav, section) h1 {
  font-size: 1.17em;
  margin-top: 1.125rem;
  margin-bottom: 1em;
}

:matches(article, aside, nav, section) :matches(article, aside, nav, section) :matches(article, aside, nav, section) h1 {
  font-size: 1.00em;
  margin-top: 1.33rem;
  margin-bottom: 1.33em;
}

:matches(article, aside, nav, section) :matches(article, aside, nav, section) :matches(article, aside, nav, section) :matches(article, aside, nav, section) h1 {
  font-size: .83em;
  margin-top: 1.67rem;
  margin-bottom: 1.67em;
}

:matches(article, aside, nav, section) :matches(article, aside, nav, section) :matches(article, aside, nav, section) :matches(article, aside, nav, section) :matches(article, de, nav, section) h1 {
  font-size: .67em;
  margin-top: 2.33rem;
  margin-bottom: 2.33em;
}

h2 {
  display: block;
  font-size: 1.5em;
  margin-top: 0.83rem;
  margin-bottom: 0.83em;
  margin-left: 0;
  margin-right: 0;
  font-weight: bold;
}

h3 {
  display: block;
  font-size: 1.17em;
  margin-top: 1.125rem;
  margin-bottom: 1em;
  margin-left: 0;
  margin-right: 0;
  font-weight: bold;
}

h4 {
  display: block;
  margin-top: 1.33rem;
  margin-bottom: 1.33em;
  margin-left: 0;
  margin-right: 0;
  font-weight: bold;
}

h5 {
  display: block;
  font-size: .83em;
  margin-top: 1.67rem;
  margin-bottom: 1.67em;
  margin-left: 0;
  margin-right: 0;
  font-weight: bold;
}

h6 {
  display: block;
  font-size: .67em;
  margin-top: 2.33rem;
  margin-bottom: 2.33em;
  margin-left: 0;
  margin-right: 0;
  font-weight: bold;
}

/* tables */

table {
  display: table;
  border-collapse: separate;
  border-spacing: 2px;
  border-color: gray;
}

thead {
  display: table-header-group;
  vertical-align: middle;
  border-color: inherit;
}

tbody {
  display: table-row-group;
  vertical-align: middle;
  border-color: inherit;
}

tfoot {
  display: table-footer-group;
  vertical-align: middle;
  border-color: inherit;
}

/* for tables without table section elements (can happen with XHTML or dynamically created tables) */
table > tr {
  vertical-align: middle;
}

col {
  display: table-column;
}

colgroup {
  display: table-column-group;
}

tr {
  display: table-row;
  vertical-align: inherit;
  border-color: inherit;
}

td, th {
  display: table-cell;
  vertical-align: inherit;
}

th {
  font-weight: bold;
}

caption {
  display: table-caption;
  text-align: center;
}

/* lists */

ul, menu, dir {
  display: block;
  list-style-type: disc;
  margin-top: 1.125rem;
  margin-bottom: 1em;
  margin-left: 0;
  margin-right: 0;
  padding-left: 40px;
}

ol {
  display: block;
  list-style-type: decimal;
  margin-top: 1.125rem;
  margin-bottom: 1em;
  margin-left: 0;
  margin-right: 0;
  padding-left: 40px;
}

li {
  display: list-item;
  text-align: inherit;
}

ul ul, ol ul {
  list-style-type: circle;
}

ol ol ul, ol ul ul, ul ol ul, ul ul ul {
  list-style-type: square;
}

dd {
  display: block;
  margin-left: 40px;
}

dl {
  display: block;
  margin-top: 1.125rem;
  margin-bottom: 1em;
  margin-left: 0;
  margin-right: 0;
}

dt {
  display: block;
}

ol ul, ul ol, ul ul, ol ol {
  margin-top: 0;
  margin-bottom: 0;
}


strong
  font-weight 700
  
em
  font-style italic